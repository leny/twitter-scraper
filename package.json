        "axios": "^0.18.0",
        "jsdom": "^13.1.0",
        "lodash.frompairs": "^4.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.12.0",
        "eslint-plugin-prettier": "^3.0.1",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.0",
        "prettier": "^1.15.3"
    }
}
